<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fredastaire.mybowlingscores">

    <application
        android:name=".MyBowlingScoresApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_dialog_my_bowling_scores"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_dialog_my_bowling_scores"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EnterScoresActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HistoryActivity"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>